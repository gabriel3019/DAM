package Ejercicios;

public class Ej2 {

	public static void main(String[] args) {
		String [][] matriz = {
		{"Pomelo", "Sol", "Acebo"},
		{"Clave", "Universo", "Tijeras"},
		{"Elefante", "luz", "Titanio"}
		};
		
		
		for (int i = 0; i < matriz.length; i++) {
			for (int j = 0; j <= i; j++) {
				if(matriz[i][j].toUpperCase() && matriz[i][j].toLowerCase() == )
				System.out.println(matriz[i][j]);
				
			}
		}
	}

}
