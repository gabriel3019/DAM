package Ejercicios;

public class Ej2 {

	/*Haz un programa que dado una matriz de strings escrito en codgio y que el resultado muestre el array sin mayusculas y minusculas*/
	public static void main(String[] args) {
		String [][] matriz = {
		{"Pomelo", "Sol", "Acebo"},
		{"Clave", "Universo", "Tijeras"},
		{"Elefante", "luz", "Titanio"}
		};
		
		
		for (int i = 0; i < matriz.length; i++) {
			for (int j = 0; j <= i; j++) {
				System.out.println(matriz[i][j]);
			}
		}
	}

}
